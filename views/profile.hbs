<!DOCTYPE html>
<html
  vocab="http://schema.org/"
  prefix="cco: http://purl.org/ontology/cco/core# foaf: http://xmlns.com/foaf/0.1/ og: http://ogp.me/ns#"
  >
<head>
  <meta http-equiv="content-type" content="text/html; charset=utf-8" />

  <title>elf Pavlik | worldwide elves</title>

  <meta property="og:type" content="profile" />
  <meta property="og:title" content="{{name}}" />
  <meta property="og:description" content="{{description}}" />
  <meta property="og:url" content="{{id}}/" />
  <meta property="og:image" content="{{image}}" />

  <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.css" />
  <link rel="stylesheet" href="/assets/screen.css" />
  <script src="/bower_components/jquery/dist/jquery.min.js"></script>
  <script src="/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

</head>
<body>
<div about="{{id}}" typeof="{{j type}}" class="container">
  <h1 property="name">{{name}}</h1>
  <img property="image" src="{{image}}" />
  <p property="description">{{description}}</p>
  <p rel="homeLocation">
    home:
    <a typeof="{{j homeLocation.type}}" href="{{homeLocation.id}}">
      <span property="name">{{homeLocation.name}}</span>
    </a>
  </p>
  <p rel="currentLocation">
    current location:
    <a typeof="{{j currentLocation.type}}" href="{{currentLocation.id}}">
      <span property="name">{{currentLocation.name}}</span>
    </a>
  </p>
  <h2>contact points</h2>
  <ul>
    {{#each contactPoint}}
    <li>
      <a rel="me" rel="contactPoint" typeof="{{j type}}" href="{{id}}"><span property="name">{{name}}</span></a>
    </li>
    {{/each}}
  </ul>
  <h2>habits</h2>
  <ul rel="cco:habit">
    {{#each cco:habit}}
    <li typeof="{{j type}}">
      <a rel="cco:topic" href="{{cco:topic.id}}" target="dbpedia">
        <span property="name">{{cco:topic.name}}</span>
      </a>
    </li>
    {{/each}}
  </ul>
  <h2>affiliations</h2>
  <ul rel="member">
    {{#each member}}
    <li>
      <a typeof="{{j type}}" href="{{id}}"><span property="name">{{name}}</span></a>
    </li>
    {{/each}}
  </ul>
  <h2>events</h2>
  <ul rev="attendee">
    {{#each attendeeIn}}
    <li>
      <a typeof="{{j type}}" href="{{id}}"><span property="name">{{name}}</span></a>
    </li>
    {{/each}}
  </ul>
  <h2>places</h2>
  <ul rel="visited">
    {{#each visited}}
    <li>
      <a typeof="{{j type}}" href="{{id}}"><span property="name">{{name}}</span></a>
    </li>
    {{/each}}
  </ul>
  <h2>wishlist</h2>
  <ul rel="seeks">
    {{#each seeks}}
    <li typeof="{{j type}}">
      <a property="itemOffered" typeof="{{j itemOffered.type}}" href="{{itemOffered.id}}"><span property="name">{{itemOffered.name}}</span></a>
    </li>
    {{/each}}
  </ul>
</div>
<footer>
  page powered by open source
  <a typeof="SoftwareApplication" href="https://github.com/elf-pavlik/webprofiled">
    <span property="name">webprofiled</span>
  </a>
</footer>
</body>
</html>
